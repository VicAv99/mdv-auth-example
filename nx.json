{
  "npmScope": "cs",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api-interfaces": {
      "tags": []
    },
    "core-data": {
      "tags": []
    },
    "core-state": {
      "tags": []
    },
    "dope-items": {
      "tags": []
    },
    "dope-items-e2e": {
      "tags": [],
      "implicitDependencies": ["dope-items"]
    },
    "environment": {
      "tags": []
    },
    "features-items": {
      "tags": []
    },
    "material": {
      "tags": []
    },
    "styles": {
      "tags": []
    }
  }
}
