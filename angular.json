{
  "version": 1,
  "projects": {
    "api-interfaces": {
      "projectType": "library",
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-interfaces"],
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/src/**/*.ts",
              "libs/api-interfaces/src/**/*.html"
            ]
          }
        }
      }
    },
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/auth"],
          "options": {
            "jestConfig": "libs/auth/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/auth/src/**/*.ts",
              "libs/auth/src/**/*.html"
            ]
          }
        }
      }
    },
    "core-data": {
      "projectType": "library",
      "root": "libs/core-data",
      "sourceRoot": "libs/core-data/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-data"],
          "options": {
            "jestConfig": "libs/core-data/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core-data/src/**/*.ts",
              "libs/core-data/src/**/*.html"
            ]
          }
        }
      }
    },
    "core-state": {
      "projectType": "library",
      "root": "libs/core-state",
      "sourceRoot": "libs/core-state/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-state"],
          "options": {
            "jestConfig": "libs/core-state/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core-state/src/**/*.ts",
              "libs/core-state/src/**/*.html"
            ]
          }
        }
      }
    },
    "dope-items": {
      "projectType": "application",
      "root": "apps/dope-items",
      "sourceRoot": "apps/dope-items/src",
      "prefix": "cs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dope-items",
            "index": "apps/dope-items/src/index.html",
            "main": "apps/dope-items/src/main.ts",
            "polyfills": "apps/dope-items/src/polyfills.ts",
            "tsConfig": "apps/dope-items/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/dope-items/src/favicon.ico",
              "apps/dope-items/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/dope-items/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/styles/src/lib"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/dope-items/src/environments/environment.ts",
                  "with": "apps/dope-items/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "dope-items:build:production"
            },
            "development": {
              "browserTarget": "dope-items:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dope-items:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dope-items/src/**/*.ts",
              "apps/dope-items/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dope-items"],
          "options": {
            "jestConfig": "apps/dope-items/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dope-items-e2e": {
      "root": "apps/dope-items-e2e",
      "sourceRoot": "apps/dope-items-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dope-items-e2e/cypress.json",
            "devServerTarget": "dope-items:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dope-items:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/dope-items-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "environment": {
      "projectType": "library",
      "root": "libs/environment",
      "sourceRoot": "libs/environment/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/environment"],
          "options": {
            "jestConfig": "libs/environment/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/environment/src/**/*.ts",
              "libs/environment/src/**/*.html"
            ]
          }
        }
      }
    },
    "features-items": {
      "projectType": "library",
      "root": "libs/features/items",
      "sourceRoot": "libs/features/items/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/features/items"],
          "options": {
            "jestConfig": "libs/features/items/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/features/items/src/**/*.ts",
              "libs/features/items/src/**/*.html"
            ]
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material"],
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/material/src/**/*.ts",
              "libs/material/src/**/*.html"
            ]
          }
        }
      }
    },
    "styles": {
      "projectType": "library",
      "root": "libs/styles",
      "sourceRoot": "libs/styles/src",
      "prefix": "cs",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/styles"],
          "options": {
            "jestConfig": "libs/styles/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/styles/src/**/*.ts",
              "libs/styles/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "dope-items"
}
